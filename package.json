{
  "name": "suit-up",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/schiehll/suit-up.git"
  },
  "keywords": [
    "react",
    "css-in-js",
    "CSS Modules",
    "postcss"
  ],
  "author": "schiehll <rn.schiehll@gmail.com> (http://github.com/schiehll)",
